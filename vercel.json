{
  "buildCommand": "echo 'Static site - no build process required'",
  "outputDirectory": "./",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": null,
  "public": true,
  "rewrites": [
    {
      "source": "/",
      "destination": "/index.html"
    },
    {
      "source": "/liquidations",
      "destination": "/liquidations.html"
    },
    {
      "source": "/docs",
      "destination": "/docs.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    },
    {
      "source": "/(.*\\.(js|css|png|jpg|jpeg|gif|svg|mp4|webm|ico))",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
